{
  "name": "install-poetry-project",
  "version": "1.2.20",
  "private": true,
  "description": "Install poetry project with cached",
  "main": "dist/index.js",
  "scripts": {
    "format": "prettier -w .",
    "lint": "eslint src/** __tests__/**",
    "build": "npx esbuild --bundle --platform=node --legal-comments=external --sourcemap=inline --target=node16 ./src/main.ts --outfile=dist/index.js",
    "test": "jest",
    "all": "npm run lint && npm run build",
    "lint-staged": "lint-staged",
    "postinstall": "husky install"
  },
  "packageManager": "yarn@3.6.3",
  "engines": {
    "node": ">=16.15.0"
  },
  "lint-staged": {
    "*.{js,ts,yaml,md,yml,json}": "prettier -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "prettier": {
    "trailingComma": "all",
    "endOfLine": "lf"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "YourNameOrOrganization",
  "license": "MIT",
  "dependencies": {
    "@actions/cache": "3.2.1",
    "@actions/core": "1.10.0",
    "@actions/exec": "1.1.1",
    "@actions/http-client": "2.1.1",
    "@renovatebot/pep440": "2.1.20"
  },
  "devDependencies": {
    "@types/jest": "29.5.4",
    "@types/node": "18.17.12",
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "@typescript-eslint/parser": "6.2.1",
    "esbuild": "0.18.17",
    "eslint": "8.46.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-standard": "17.1.0",
    "eslint-config-standard-with-typescript": "39.0.0",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-n": "16.0.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-standard": "5.0.0",
    "husky": "8.0.3",
    "jest": "29.6.2",
    "jest-circus": "29.6.2",
    "lint-staged": "14.0.1",
    "prettier": "3.0.0",
    "ts-jest": "29.1.1",
    "typescript": "5.1.6"
  }
}
