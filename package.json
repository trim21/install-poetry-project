{
  "name": "install-poetry-project",
  "version": "2.1.0",
  "private": true,
  "description": "Install poetry project with cached",
  "main": "dist/index.js",
  "scripts": {
    "format": "prettier -w .",
    "lint": "eslint src/** __tests__/**",
    "build": "npx esbuild --bundle --platform=node --packages=bundle --legal-comments=external --sourcemap=inline --target=node20 ./src/main.ts --outfile=dist/index.js",
    "test": "jest",
    "all": "npm run lint && npm run build",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "packageManager": "pnpm@10.5.2",
  "engines": {
    "node": "^20"
  },
  "lint-staged": {
    "*.{js,ts,yaml,md,yml,json}": "prettier -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "prettier": {
    "trailingComma": "all",
    "endOfLine": "lf"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "YourNameOrOrganization",
  "license": "MIT",
  "dependencies": {
    "@actions/cache": "4.0.2",
    "@actions/core": "1.11.1",
    "@actions/exec": "1.1.1",
    "@actions/http-client": "2.2.3",
    "@renovatebot/pep440": "4.1.0"
  },
  "devDependencies": {
    "@types/jest": "29.5.14",
    "@types/node": "20.17.22",
    "@typescript-eslint/eslint-plugin": "8.25.0",
    "@typescript-eslint/parser": "8.25.0",
    "esbuild": "0.25.0",
    "eslint": "9.22.0",
    "eslint-config-prettier": "10.0.2",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jest": "28.11.0",
    "eslint-plugin-n": "17.15.1",
    "eslint-plugin-promise": "7.2.1",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-circus": "29.7.0",
    "lint-staged": "15.4.3",
    "prettier": "3.5.2",
    "ts-jest": "29.2.6",
    "typescript": "5.8.2"
  }
}
