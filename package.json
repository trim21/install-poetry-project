{
  "name": "install-poetry-project",
  "version": "2.0.0",
  "private": true,
  "description": "Install poetry project with cached",
  "main": "dist/index.js",
  "scripts": {
    "format": "prettier -w .",
    "lint": "eslint src/** __tests__/**",
    "build": "npx esbuild --bundle --platform=node --legal-comments=external --sourcemap=inline --target=node16 ./src/main.ts --outfile=dist/index.js",
    "test": "jest",
    "all": "npm run lint && npm run build",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "packageManager": "pnpm@9.0.6",
  "engines": {
    "node": "^20"
  },
  "lint-staged": {
    "*.{js,ts,yaml,md,yml,json}": "prettier -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "prettier": {
    "trailingComma": "all",
    "endOfLine": "lf"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "YourNameOrOrganization",
  "license": "MIT",
  "dependencies": {
    "@actions/cache": "3.2.4",
    "@actions/core": "1.10.1",
    "@actions/exec": "1.1.1",
    "@actions/http-client": "2.2.1",
    "@renovatebot/pep440": "3.0.20"
  },
  "devDependencies": {
    "@types/jest": "29.5.12",
    "@types/node": "20.12.7",
    "@typescript-eslint/eslint-plugin": "7.8.0",
    "@typescript-eslint/parser": "7.8.0",
    "esbuild": "0.20.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-config-love": "46.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "28.3.0",
    "eslint-plugin-n": "17.4.0",
    "eslint-plugin-promise": "6.1.1",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "jest-circus": "29.7.0",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "ts-jest": "29.1.2",
    "typescript": "5.4.3"
  }
}
